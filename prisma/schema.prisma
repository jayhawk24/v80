generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["jsonProtocol"]
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Stock {
  id      String @id @default(cuid())
  name    String
  symbol  String
  price   Float
  change  Float
  holding Int
}

model UserStocks {
  id      String @id @default(cuid())
  userId  String
  stockId String
}
